<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<!DOCTYPE parameters [
 <!ELEMENT parameters (param+) >
 <!ELEMENT param (label?, description?, info?, value?, param*) >
 <!ELEMENT label (#PCDATA)>
 <!ELEMENT description (#PCDATA)>
 <!ELEMENT info (#PCDATA)>
 <!ELEMENT value (#PCDATA)>
 <!ATTLIST param name NMTOKEN #REQUIRED>
 <!ATTLIST param type NMTOKEN #REQUIRED>
 <!ATTLIST param type ( String | Boolean | Integer | Float | Double | Option | Collection ) #REQUIRED>
 <!ATTLIST param view_x NMTOKEN #IMPLIED>
 <!ATTLIST param view_y NMTOKEN #IMPLIED>
 <!ATTLIST param view_width NMTOKEN #IMPLIED>
 <!ATTLIST param view_height NMTOKEN #IMPLIED>
]>

<parameters>

  <!-- ====== start of collection Root ====== -->

  <param name="Root" type="Collection" view_x="50" view_y="50" view_width="390" view_height="685">
    <label>Param manager</label>
    <description>Root-depth parameter manager</description>

    <param name="debug" type="Boolean">
      <label>Debugging flag</label>
      <description></description>
      <value>true</value>
    </param>

    <param name="verbose" type="Boolean">
      <label>Verbose debugging flag</label>
      <description></description>
      <value>false</value>
    </param>

    <!-- ====== start of collection radar ====== -->

    <param name="radar" type="Collection" view_x="-1" view_y="-1" view_width="517" view_height="533">
      <label>Radar params</label>
      <description>Edit the radar parameters</description>

      <param name="siteNum" type="Integer">
        <label>Site number</label>
        <description>The indexed number for this site</description>
        <info>
          <![CDATA[
          You should arrange these numbers with reference to other sites in the area. The number is used identify the radar site if mode than one radar is used in the network
          ]]>
        </info>
        <value>0</value>
      </param>

      <param name="siteName" type="String">
        <label>Site name</label>
        <description>The name of the site</description>
        <info>
          <![CDATA[
          The site name must be a text description of the geographical geographical location of the radar installation, such as a farm name or nearby town.
          ]]>
        </info>
        <value>Denver</value>
      </param>

      <param name="userName" type="String">
        <label>User name</label>
        <description>The name of the technician or user</description>
        <info>
          <![CDATA[
          The technician or user of the program can put their name in this field. The reason for having it is to keep track of who performed the last calibration.
          ]]>
        </info>
        <value>Unknown</value>
      </param>

      <param name="latitude" type="Double">
        <label>Latitude</label>
        <description>Radar latitude (decimal deg)</description>
        <value>40.0</value>
      </param>

      <param name="longitude" type="Double">
        <label>Longitude</label>
        <description>Radar longitude (decimal deg)</description>
        <value>-105.0</value>
      </param>

      <param name="altitude" type="Double">
        <label>Altitude</label>
        <description>Radar altitude (km)</description>
        <value>1.7</value>
      </param>

      <param name="frequency" type="Double">
        <label>Frequency (GHz)</label>
        <description>Radar frequency (GHz)</description>
        <value>5.5</value>
      </param>

      <param name="horizBeamWidth" type="Double">
        <label>Horiz beam width</label>
        <description>Antenna horizontal beam width (deg)</description>
        <info>
          <![CDATA[
          Half-power horizontal beam width in degrees
          ]]>
        </info>
        <value>1.0</value>
      </param>

      <param name="vertBeamWidth" type="Double">
        <label>Vert beam width</label>
        <description>Antenna vertical beam width (deg)</description>
        <info>
          <![CDATA[
          Half-power vertical beam width in degrees
          ]]>
        </info>
        <value>1.0</value>
      </param>

      <param name="antGain" type="Double">
        <label>Antenna gain</label>
        <description>Antenna gain (dB)</description>
        <info>
          <![CDATA[
          Antenna gain in dB
          ]]>
        </info>
        <value>44.0</value>
      </param>

    </param>

    <!-- ======= end of collection radar ======= -->

    <!-- ====== start of collection scan ====== -->

    <param name="scan" type="Collection" view_x="-1" view_y="-1" view_width="515" view_height="542">
      <label>Scan strategy</label>
      <description>Edit the RDAS comm parameters</description>

      <param name="elevationList" type="String">
        <label>Elevation list</label>
        <description>Comma-delimited elevation angle list</description>
        <info>
          <![CDATA[
          Enter the elevation angles in a comma-delimited list.
          ]]>
        </info>
        <value>0.5, 1.5, 2.5, 3.5, 4.5, 5.5, 6.5, 7.5, 8.5, 9.9</value>
      </param>

      <param name="azSlewRate" type="Double">
        <label>Azimuth slew rate</label>
        <description>Antenna slew rate in azimuth (deg/sec)</description>
        <value>19.0</value>
      </param>

      <param name="elSlewRate" type="Double">
        <label>Elevation slew rate</label>
        <description>Antenna slew rate in elevation (deg/sec)</description>
        <value>9.0</value>
      </param>

      <param name="nGates" type="Integer">
        <label>N Gates</label>
        <description>Number of gates per beam</description>
        <value>500</value>
      </param>

      <param name="startRange" type="Double">
        <label>Start range</label>
        <description>Range to center of first gate (km)</description>
        <value>0.125</value>
      </param>

      <param name="gateSpacing" type="Double">
        <label>Gate spacing</label>
        <description>Spacing between gates (km)</description>
        <value>0.25</value>
      </param>

      <param name="prf" type="Double">
        <label>PRF</label>
        <description>Pulse Repetition Frequency (/s)</description>
        <value>1000.0</value>
      </param>

      <param name="samplesPerAz" type="Integer">
        <label>N samples per az</label>
        <description>Number of samples per az</description>
        <info>
          <![CDATA[
          If more than 1, this implies averaging in azimuth
          ]]>
        </info>
        <value>8</value>
      </param>

      <param name="samplesPerGate" type="Integer">
        <label>N samples per gate</label>
        <description>Number of samples per gate</description>
        <info>
          <![CDATA[
          If more than 1, this implies averaging in range
          ]]>
        </info>
        <value>4</value>
      </param>

      <param name="polarization" type="Option">
        <label>Polarization</label>
        <description>Polarization type</description>
        <value>Horizontal</value>
      </param>

    </param>

    <!-- ======= end of collection scan ======= -->

    <!-- ====== start of collection rdasComm ====== -->

    <param name="rdasComm" type="Collection" view_x="100" view_y="100" view_width="573" view_height="709">
      <label>RDAS socket</label>
      <description></description>

      <param name="connectAtStartup" type="Boolean">
        <label>Connect at startup</label>
        <description>Connect to RDAS or relay at startup.</description>
        <info>
          <![CDATA[
          If TRUE, the socket connection to either RDAS or RELAY will be set up at startup. The connection can be open or closed using the COMMS menu.
          ]]>
        </info>
        <value>true</value>
      </param>

      <param name="host" type="String">
        <label>RDAS host</label>
        <description>IP name of RDAS host</description>
        <info>
          <![CDATA[
          This can be either a hostname or IP address.
          ]]>
        </info>
        <value>localhost</value>
      </param>

      <param name="port" type="Integer">
        <label>RDAS port</label>
        <description>TCP/IP port for RDAS server</description>
        <value>10000</value>
      </param>

      <param name="relayHost" type="String">
        <label>RDAS relay host</label>
        <description>IP name of host of RDAS relay service</description>
        <info>
          <![CDATA[
          This can be either a hostname or IP address.
          ]]>
        </info>
        <value>localhost</value>
      </param>

      <param name="relayPort" type="Integer">
        <label>RDAS relay port</label>
        <description>TCP/IP port for RDAS relay service</description>
        <value>11000</value>
      </param>

      <param name="viaRelay" type="Boolean">
        <label>Contact relay server?</label>
        <description>Option to contact relay host.</description>
        <info>
          <![CDATA[
          If FALSE, contacts RDAS directly. If true, contacts relay host
          ]]>
        </info>
        <value>false</value>
      </param>

      <param name="timeBetweenRelayBeams" type="Double">
        <label>Time between beams (secs)</label>
        <description>Time between beams when getting data via relay</description>
        <info>
          <![CDATA[
          Normally, every available beam is sent from the relay server to the client. However, if network speed is limited this may not be practical. If you set this parameter to a value greater than 0, in seconds, beams will only be sent at this interval. Note that the value does not need to be an integer - a value of 0.5 means a beam will be sent every half second. The default is 0, which means every beam will be sent.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="acceptRemoteCommands" type="Boolean">
        <label>Accept remote commands?</label>
        <description>Accept commands in relay mode.</description>
        <info>
          <![CDATA[
          Only applies to relay mode. If TRUE, will accept commands from a remote instance of RdasControl. If FALSE, will not accept remote commands. If FALSE, the only way to shut down the radar is to terminate the relay application.
          ]]>
        </info>
        <value>true</value>
      </param>

      <param name="maxDataQueueSize" type="Integer">
        <label>Max data queue size</label>
        <description>Max size of incoming radar data queue</description>
        <info>
          <![CDATA[
          This is the maximum number of messages which can be waiting for attention at any time. Limiting the size prevents memory usage from growing in an uncontrolled manner if an error occurs within the program.
          ]]>
        </info>
        <value>1000</value>
      </param>

      <param name="maxCommandQueueSize" type="Integer">
        <label>Max command queue size</label>
        <description>Max size of command queue</description>
        <info>
          <![CDATA[
          This is the maximum number of commands which can be waiting to be sent at any time.
          ]]>
        </info>
        <value>100</value>
      </param>

      <param name="connectDelay" type="Integer">
        <label>Connect delay (msecs)</label>
        <description>Delay after connection established - msecs</description>
        <info>
          <![CDATA[
          This is the delay after the connection is established before any commands are sent to RDAS.
          ]]>
        </info>
        <value>500</value>
      </param>

      <param name="commandDelay" type="Integer">
        <label>Command delay (msecs)</label>
        <description>Delay before message is sent - msecs</description>
        <info>
          <![CDATA[
          For debugging. You can set this to a value greater than 0 to cause a delay before each command is sent to RDAS.
          ]]>
        </info>
        <value>100</value>
      </param>

      <param name="bigEndian" type="Boolean">
        <label>Big-endian data?</label>
        <description>Is data in BE - network byte order - format?</description>
        <info>
          <![CDATA[
          If FALSE, data is in little-endian format.
          ]]>
        </info>
        <value>true</value>
      </param>

    </param>

    <!-- ======= end of collection rdasComm ======= -->

    <!-- ====== start of collection calib ====== -->

    <param name="calib" type="Collection" view_x="100" view_y="100" view_width="574" view_height="1075">
      <label>Calibration parameters</label>
      <description>Edit the calibration values</description>

      <param name="fileFolder" type="String">
        <label>Calibration file folder</label>
        <description>Folder in which to save and retrieve calibration files</description>
        <value>./calib</value>
      </param>

      <param name="filePath" type="String">
        <label>Calibration file path</label>
        <description>Current calibration file</description>
        <value>./calib/20041206_013027.cal.xml</value>
      </param>

      <param name="peakPower" type="Double">
        <label>Peak power (dBm)</label>
        <description>Peak transmitter power (dBm)</description>
        <value>84.0</value>
      </param>

      <param name="pulseWidth" type="Double">
        <label>Pulse width</label>
        <description>Pulse width (us)</description>
        <value>2.0</value>
      </param>

      <param name="waveguideLoss" type="Double">
        <label>Waveguide loss</label>
        <description>One-way waveguide loss (dB)</description>
        <info>
          <![CDATA[
          Typical loss values:
<p>  C-band: 1.75 dB per 100 ft / 30 m
<p>  S-band: 0.75 dB per 100 ft / 30 m

          ]]>
        </info>
        <value>0.5</value>
      </param>

      <param name="radomeLoss" type="Double">
        <label>Radome loss</label>
        <description>Two-way radome loss (dB)</description>
        <info>
          <![CDATA[
          If no radome set to 0.
          ]]>
        </info>
        <value>0.5</value>
      </param>

      <param name="receiverLoss" type="Double">
        <label>Receiver loss</label>
        <description>Receiver bandwidth loss (dB)</description>
        <info>
          <![CDATA[
          Loss due to the effective bandpass filter associated with the receiver (dB)
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="testCableAtten" type="Double">
        <label>Test cable attenuation</label>
        <description>Test cable attenuation (dB)</description>
        <info>
          <![CDATA[
          Typical values:
<p>C-band: 0.6 dB/ft with RG58 or similar
<p>S-band: 0.4 dB/ft with RG58 or similar

          ]]>
        </info>
        <value>3.8</value>
      </param>

      <param name="couplerAtten" type="Double">
        <label>Coupler attenuation</label>
        <description>Coupler attenuation (dB)</description>
        <info>
          <![CDATA[
          If the signal generator input is connected directly to the receiver set this to 0
          ]]>
        </info>
        <value>30.75</value>
      </param>

      <param name="nPointsTable" type="Integer">
        <label>N Points Table</label>
        <description>Number of points in calibration table</description>
        <info>
          <![CDATA[
          This is the number of available points in the table. Not all of the points are necessarily used.
          ]]>
        </info>
        <value>15</value>
      </param>

      <param name="minDbz" type="Double">
        <label>Min dBZ</label>
        <description>Minimim dBZ for sampling</description>
        <info>
          <![CDATA[
          This the min dBZ value in the calibration table
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="deltaDbz" type="Double">
        <label>Delta dBZ</label>
        <description>dBZ step for calib table</description>
        <info>
          <![CDATA[
          This the dBZ step in the calibration table
          ]]>
        </info>
        <value>5.0</value>
      </param>

      <param name="fitDbzLow" type="Double">
        <label>Fit dBZ low</label>
        <description>Low dBZ limit for linear fit</description>
        <info>
          <![CDATA[
          The linear fit is computed for points which lie between the low and high dBZ limits
          ]]>
        </info>
        <value>5.0</value>
      </param>

      <param name="fitDbzHigh" type="Double">
        <label>Fit dBZ high</label>
        <description>High dBZ limit for linear fit</description>
        <info>
          <![CDATA[
          The fit is computed for points which lie between the low and high dBZ limits
          ]]>
        </info>
        <value>65.0</value>
      </param>

      <param name="calRange" type="Double">
        <label>Calib range (km)</label>
        <description>Range for calibration test pulse (km)</description>
        <info>
          <![CDATA[
          The is the effective range for the test pulse. Final calibration is computed for 1km
          ]]>
        </info>
        <value>100.0</value>
      </param>

      <param name="roundSiggen" type="Boolean">
        <label>Round siggen?</label>
        <description>If true, the siggen values will be rounded to the nearest 'siggenRoundValue'. Other columns in the table will be adjusted accordingly.</description>
        <value>true</value>
      </param>

      <param name="siggenRoundValue" type="Double">
        <label>Siggen round value</label>
        <description>Val to which siggen vals are rounded</description>
        <info>
          <![CDATA[
          See roundSiggen
          ]]>
        </info>
        <value>5.0</value>
      </param>

      <param name="slope" type="Double">
        <label>Slope</label>
        <description>Slope of calibration line</description>
        <value>118.5948</value>
      </param>

      <param name="offset1km" type="Double">
        <label>Offset-1km</label>
        <description>Offset (in counts) for dBZ at 1km</description>
        <value>4730.227</value>
      </param>

      <param name="offsetCalRng" type="Double">
        <label>Offset at cal range</label>
        <description>Offset (in counts) for dBZ at calib range</description>
        <value>-178.4109</value>
      </param>

      <param name="topMargin" type="Integer">
        <label>Top margin</label>
        <description>Top margin (pixels)</description>
        <value>25</value>
      </param>

      <param name="bottomMargin" type="Integer">
        <label>Bottom margin</label>
        <description>Bottom margin (pixels)</description>
        <value>30</value>
      </param>

      <param name="leftMargin" type="Integer">
        <label>Left margin</label>
        <description>Left margin (pixels)</description>
        <value>45</value>
      </param>

      <param name="rightMargin" type="Integer">
        <label>Right margin</label>
        <description>Right margin (pixels)</description>
        <value>35</value>
      </param>

      <param name="xx" type="Integer">
        <label>X offset</label>
        <description>The X offset from the parent</description>
        <info>
          <![CDATA[
          This the the X offset, in pixels, from the main frame of the application.
          ]]>
        </info>
        <value>601</value>
      </param>

      <param name="yy" type="Integer">
        <label>Y offset</label>
        <description>The Y offset from the parent</description>
        <info>
          <![CDATA[
          This the the Y offset, in pixels, from the main frame of the application.
          ]]>
        </info>
        <value>232</value>
      </param>

      <param name="width" type="Integer">
        <label>Width</label>
        <description>The window width in pixels</description>
        <value>573</value>
      </param>

      <param name="height" type="Integer">
        <label>Height</label>
        <description>The window height in pixels</description>
        <value>769</value>
      </param>

      <param name="tableRowHt" type="Integer">
        <label>TableRowHeight</label>
        <description>Height of a table row in pixels</description>
        <info>
          <![CDATA[
          Adjust so that the table fits nicely into the viewport and just does not scroll.
          ]]>
        </info>
        <value>16</value>
      </param>

      <param name="startVisible" type="Boolean">
        <label>Start visible</label>
        <description>Start with the window visible?</description>
        <info>
          <![CDATA[
          If true, the program will start with the PPI visible. If not, it will be hidden at startup
          ]]>
        </info>
        <value>false</value>
      </param>

      <param name="testPath" type="File">
        <label>Test path</label>
        <description>Path for test file</description>
        <value>unknown</value>
      </param>

    </param>

    <!-- ======= end of collection calib ======= -->

    <!-- ====== start of collection eng ====== -->

    <param name="eng" type="Collection" view_x="100" view_y="100" view_width="583" view_height="672">
      <label>Engineering parameters</label>
      <description>Edit the engineering values</description>

      <param name="maxCount" type="Integer">
        <label>Max count</label>
        <description>Max count possible from RDAS</description>
        <value>16384</value>
      </param>

      <param name="autoResetTimeout" type="Integer">
        <label>Auto-reset timeout</label>
        <description>Timeout (s) to trigger auto-reset.</description>
        <info>
          <![CDATA[
          Sometimes the system will lock up and require resetting. If the system locks up for more than this timeout values, in secs, a reset will be triggered.
          ]]>
        </info>
        <value>10</value>
      </param>

      <param name="elevTolerance" type="Double">
        <label>Elev tolerance</label>
        <description>Elevation control tolerance (deg).</description>
        <info>
          <![CDATA[
          The elevation angle must be withing this tolerance of the theoretical value before a PPI can start.
          ]]>
        </info>
        <value>0.31</value>
      </param>

      <param name="ppiAzOverlap" type="Double">
        <label>PPI azimuth overlap</label>
        <description>Azimuth overlap for full PPI (deg)</description>
        <info>
          <![CDATA[
          In order to complete a full PPI, some overlap is required to avoid missing beams. This is the overlap which must be reached before a PPI will complete.
          ]]>
        </info>
        <value>10.0</value>
      </param>

      <param name="antennaElCorr" type="Double">
        <label>Antenna El Correction</label>
        <description>Correction for pedestal antenna position in elevation (decimal deg)</description>
        <info>
          <![CDATA[
          This value is added to the pedestal readout to correct the elevation value.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="antennaAzCorr" type="Double">
        <label>Antenna Az Correction</label>
        <description>Correction for pedestal antenna position in azimuth (decimal deg)</description>
        <info>
          <![CDATA[
          This value is added to the pedestal readout to correct the azimuth value.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="controlElCorr" type="Double">
        <label>Control El Correction</label>
        <description>Correction for driving antenna in elevation (decimal deg)</description>
        <info>
          <![CDATA[
          If the antenna elevation readout does not match the intended value, correct it by inserting a correction value here.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="controlAzCorr" type="Double">
        <label>Control Az Correction</label>
        <description>Correction for driving antenna in azimuth (decimal deg)</description>
        <info>
          <![CDATA[
          If the antenna azimuth readout does not match the intended value, correct it by inserting a correction value here.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="antennaMinElev" type="Double">
        <label>Antenna min el</label>
        <description>Min antenna elevation (deg)</description>
        <info>
          <![CDATA[
          The antenna cannot go below this elevation. Therefore the control program will not request elevation angles below this value.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="antennaMaxElev" type="Double">
        <label>Antenna max el</label>
        <description>Max antenna elevation (deg)</description>
        <info>
          <![CDATA[
          The antenna cannot go above this elevation. Therefore the control program will not request elevation angles above this value.
          ]]>
        </info>
        <value>45.0</value>
      </param>

      <param name="antennaMaxElSlewRate" type="Double">
        <label>Antenna max el slew rate</label>
        <description>Max el slew rate (deg/s)</description>
        <info>
          <![CDATA[
          The antenna cannot slew faster than this. Therefore the control program will not request slew rates above this value.
          ]]>
        </info>
        <value>10.0</value>
      </param>

      <param name="antennaMaxAzSlewRate" type="Double">
        <label>Antenna max az slew rate</label>
        <description>Max az slew rate (deg/s)</description>
        <info>
          <![CDATA[
          The antenna cannot slew faster than this. Therefore the control program will not request slew rates above this value.
          ]]>
        </info>
        <value>24.0</value>
      </param>

      <param name="elVoltagePositive" type="Boolean">
        <label>El voltage positive?</label>
        <description>Is the el control voltage sense positive?</description>
        <info>
          <![CDATA[
          If the el control voltage sense is negative, set this to false.
          ]]>
        </info>
        <value>true</value>
      </param>

      <param name="azVoltagePositive" type="Boolean">
        <label>Az voltage positive?</label>
        <description>Is the az control voltage sense positive?</description>
        <info>
          <![CDATA[
          If the az control voltage sense is negative, set this to false.
          ]]>
        </info>
        <value>true</value>
      </param>

    </param>

    <!-- ======= end of collection eng ======= -->

    <!-- ====== start of collection aScope ====== -->

    <param name="aScope" type="Collection" view_x="100" view_y="100" view_width="525" view_height="845">
      <label>A-Scope</label>
      <description>A-scope display - for calibration</description>
      <info>
        <![CDATA[
        Parameters for the size, location, etc. of the A-scope
        ]]>
      </info>

      <param name="dbzColor" type="Color">
        <label>dBZ plot color</label>
        <description>Color of dBZ line on plot.</description>
