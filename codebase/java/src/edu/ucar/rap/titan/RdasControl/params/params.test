    <param name="warningLog" type="Collection" view_x="100" view_y="100" view_width="534" view_height="288">
      <label>Warning log parameters</label>
      <description>Edit the warning log params</description>

      <param name="loggingActive" type="Boolean">
        <label>Is logging active?</label>
        <description>Should warnings be logged to file?</description>
        <info>
          <![CDATA[
          If true, warnings will be logged to a file when an error occurs or the error state changes.
          ]]>
        </info>
        <value>true</value>
      </param>

      <param name="logChangeToNormal" type="Boolean">
        <label>Log change back to normal?</label>
        <description>Log change from error state back to normal?</description>
        <info>
          <![CDATA[
          If true, a log message will be generated when the status returns from error to normal. If false, log messages will only be generated when (a) a state change occurs and (b) an error condition still exists.
          ]]>
        </info>
        <value>true</value>
      </param>

      <param name="minSecsOperateMode" type="Integer">
        <label>Min secs in Operate mode</label>
        <description>Min number of secs in operate mode before logging starts.</description>
        <info>
          <![CDATA[
          Warnings should only be logged in Operate mode. This is the number of seconds in which the system must be in Operate mode before we start logging warnings.
          ]]>
        </info>
        <value>5</value>
      </param>

      <param name="minSecsSteady" type="Integer">
        <label>Min secs in steady state</label>
        <description>Min number of secs in steady state before warnings are logged.</description>
        <info>
          <![CDATA[
          Warnings should only be logged if the warning state remains steady. This is the minimum number of seconds for the warning state to be steady before a warnings are logged.
          ]]>
        </info>
        <value>10</value>
      </param>

      <param name="fileFolder" type="String">
        <label>Warning logs file folder</label>
        <description>Folder in which to save and warning logs</description>
        <value>./warnings</value>
      </param>

